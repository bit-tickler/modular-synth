{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env*.local"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".svelte-kit/**", "build/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {},
    "type-check": {
      "dependsOn": ["^build"],
      "outputs": []
    }
  }
}
